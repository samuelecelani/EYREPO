# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# We recommend you to keep these unchanged
[*]
end_of_line = crlf
charset = utf-8

trim_trailing_whitespace = true
insert_final_newline = true

indent_size = 2
indent_style = space

max_line_length = 120

[*.java]
indent_size = 4

[*.md]
trim_trailing_whitespace = false
